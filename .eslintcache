n//   );\n\n//   return (\n//     <div className={classes.users}>\n//       <button onClick={toggleUsersHandler}>\n//         {showUsers ? 'Hide' : 'Show'} Users\n//       </button>\n//       {showUsers && usersList}\n//     </div>\n//   );\n// };\n\nexport default Users;\n",["30","31"],"/Users/markuz/WEB/CA/TYPE1/React/React_16/React-class-components-intro/src/components/User.js",[],"/Users/markuz/WEB/CA/TYPE1/React/React_16/React-class-components-intro/src/components/UserFinder.js",["32"],"import css from './UserFinder.module.css';\nimport { useState, useEffect, Component } from 'react';\nimport Users from './Users';\n\nconst DUMMY_USERS = [\n  { id: 'u1', name: 'James' },\n  { id: 'u2', name: 'Mike' },\n  { id: 'u3', name: 'Jane' },\n];\n\nclass UserFinder extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      filteredUsers: [],\n      searchTerm: '',\n    };\n    console.log('constructor');\n  }\n\n  componentDidMount() {\n    console.log('componentDidMount');\n    this.setState({ filteredUsers: DUMMY_USERS });\n  }\n\n  componentDidUpdate(prevProps, prevState) {\n    console.log('componentDidUpdate');\n    // if searchTerm changed i will update\n    if (prevState.searchTerm !== this.state.searchTerm) {\n      const filteredArr = DUMMY_USERS.filter((userObj) =>\n        userObj.name.includes(this.state.searchTerm)\n      );\n      this.setState({ filteredUsers: filteredArr });\n    }\n    // infinite loop\n  }\n\n  // mehods\n  searchChangeHandler = (e) => {\n    this.setState({ searchTerm: e.target.value });\n  };\n  render() {\n    return (\n      <>\n        <div className={css.finder}>\n          <input\n            type='search'\n            value={this.state.searchTerm}\n            onChange={this.searchChangeHandler}\n          />\n          {this.state.searchTerm && (\n            <h3>You have searched for: \"{this.state.searchTerm}\"</h3>\n          )}\n        </div>\n        <Users users={this.state.filteredUsers} />\n      </>\n    );\n  }\n}\n// CLASS END\n\nfunction UserFinder1() {\n  const [filteredUsers, setFilteredUsers] = useState([]);\n  const [searchTerm, setSearchTerm] = useState('');\n\n  const searchChangeHandler = (e) => {\n    setSearchTerm(e.target.value);\n  };\n  // componentDidMount = useEffect(() => {}, [])\n  useEffect(() => {\n    setFilteredUsers(DUMMY_USERS);\n    // componentWillUnmount()\n    return () => console.log('clenup');\n  }, []);\n\n  // componentDidUpdate - class based\n  useEffect(() => {\n    console.log('searchTerm updated', searchTerm);\n    const filteredArr = DUMMY_USERS.filter((userObj) =>\n      userObj.name.includes(searchTerm)\n    );\n    setFilteredUsers(filteredArr);\n  }, [searchTerm]);\n\n  return (\n    <>\n      <div className={css.finder}>\n        <input\n          type='search'\n          value={searchTerm}\n          onChange={searchChangeHandler}\n        />\n      </div>\n      <Users users={filteredUsers} />\n    </>\n  );\n}\n\nexport default UserFinder;\n","/Users/markuz/WEB/CA/TYPE1/React/React_16/React-class-components-intro/src/store/UsersContext.js",[],{"ruleId":"33","severity":1,"message":"34","line":1,"column":10,"nodeType":"35","messageId":"36","endLine":1,"endColumn":18},{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","replacedBy":"40"},{"ruleId":"33","severity":1,"message":"41","line":62,"column":10,"nodeType":"35","messageId":"36","endLine":62,"endColumn":21},"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","no-native-reassign",["42"],"no-negated-in-lhs",["43"],"'UserFinder1' is defined but never used.","no-global-assign","no-unsafe-negation"]